{
  "name": "kartavya-backend",
  "version": "1.0.0",
  "description": "Backend API for Kartavya Project Management System",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node scripts/sql-seed.js",
    "db:export": "node scripts/export-db.js",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "project-management",
    "jira",
    "agile",
    "scrum",
    "kanban"
  ],
  "author": "Kartavya Team",
  "license": "MIT"
}
